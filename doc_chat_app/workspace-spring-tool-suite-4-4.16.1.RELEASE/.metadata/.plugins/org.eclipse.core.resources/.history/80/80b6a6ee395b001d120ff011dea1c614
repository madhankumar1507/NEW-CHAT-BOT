package com.example.demo.Controllers;

import java.awt.PageAttributes.MediaType;

import org.springframework.web.bind.annotation.CrossOrigin;
import org.springframework.web.bind.annotation.PostMapping;
import org.springframework.web.bind.annotation.RequestBody;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.ResponseBody;
import org.springframework.web.bind.annotation.RestController;

import com.example.demo.Beans.LoginRequest;

@CrossOrigin("*")
@RestController
@RequestMapping("/v1")
public class LoginController {
	@PostMapping(value = "Login", produces = MediaType.APPLICATION_JSON_VALUE, consumes = MediaType.APPLICATON_JSON_VALUE)
	public @ResponseBody String Login(@RequestBody LoginRequest request) {
		
	}
}
